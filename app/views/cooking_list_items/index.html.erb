<h1>Cooking_List_Items#index</h1>

<% @cooking_list_items.each do |item| %>
    <h2><%= item.recipe.name %></h2>
    <p><%= item.recipe.prep_time %></p>
    <p><%= item.recipe.cook_time %></p>
    <p><%= item.recipe.servings %></p>
    <p><%= item.recipe.rating %></p>
    <%= link_to recipe_path(item.recipe.id)%>
<% end %>
